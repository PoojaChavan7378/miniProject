                                                <option>Electrical</option>
                                                <option>Electronics and Accessories</option>
                                                <option>Stationary</option>
                                                <option>Hardware</option>
                                                <option>Automobile Accessories</option>
                                                <option>Cosmetics</option>
                                                <option>Sports and Fitness</option>
                                                <option>Furniture</option>
                                                <option>Cutlery</option>
                                                <option>Gift Shop</option>
                                                <option>Toys</option>
                                                <option>Bags and Luggage</option>
                                                <option>Footware</option>
                                                <option>Spectacles/Lenses</option>
                                                <option>Watch</option>
                                                <option>Books</option>
                                                <option>Imitation Jewellery</option>
                                                <option>Baby Care</option>


                                            </select>
                                        </div>

                                        <div class="col-md-6 first-page-valid">
                                            <label for="inputPassword4 " class="form-label">Business Mobile
                                                No</label>
                                            <input type="tel" name="bMobile" class="form-control" pattern="[0-9]{10}" required>
                                            <div class="invalid-feedback">Mobile No must contain 10 digits.</div>
