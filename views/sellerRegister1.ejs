                                <div id="input-grp-double-1" class="form-row mt-4">
                                    <div class="row g-3">
                                        <div class="col-md-6 first-page-valid">
                                            <label for="inputEmail4" class="form-label">Business Name</label>
                                            <input type="text" name="bName" value="<%=b_name%>" class="form-control" pattern="[A-Za-z\s]*" required readonly>
                                            <div class="invalid-feedback">Name must contain alphabets only.</div>
